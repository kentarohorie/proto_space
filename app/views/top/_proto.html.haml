.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to '#' do
      = image_tag proto.thumbnails[0][:image]
      .caption
        %h3
          = proto.title
          .proto-meta
            .proto-user
              = link_to 'by ' + proto.user.username, '#'
            .proto-posted
              = proto.get_month_english(proto.updated_at.month)
          %ul.proto-tag-list.list-inline
            %li
              = link_to 'iPad', '#', class: 'btn btn-default'
            %li
              = link_to 'wireframe', '#', class: 'btn btn-default'